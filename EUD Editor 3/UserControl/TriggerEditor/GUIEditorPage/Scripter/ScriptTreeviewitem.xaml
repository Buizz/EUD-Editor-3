<UserControl x:Class="ScriptTreeviewitem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EUD_Editor_3"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             PreviewMouseDown="UserControl_PreviewMouseDown" PreviewMouseUp="UserControl_PreviewMouseUp">

    <Grid Margin="0,-6">
        <Grid x:Name="ColorBorder" Margin="-8,-8"/>
        <Border x:Name="color" Margin="-8" HorizontalAlignment="Left">
            <TextBlock x:Name="textblock" HorizontalAlignment="Left" Margin="8" >
            </TextBlock>
        </Border>
    </Grid>
</UserControl>
