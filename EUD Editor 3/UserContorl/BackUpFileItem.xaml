<UserControl x:Class="BackUpFileItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EUD_Editor_3"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel LastChildFill="False">
        <TextBlock x:Name="FileName" Text="aaa1" DockPanel.Dock="Top" FontSize="16" HorizontalAlignment="Right"/>
        <TextBlock x:Name="FileDate" Text="aaa2" DockPanel.Dock="Bottom" TextWrapping="Wrap" HorizontalAlignment="Right"/>
    </DockPanel>
</UserControl>
