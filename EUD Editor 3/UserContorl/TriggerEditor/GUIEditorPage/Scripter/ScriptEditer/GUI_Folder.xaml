<UserControl x:Class="GUI_Folder"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EUD_Editor_3"
             mc:Ignorable="d" 
             Width="200" Height="56">
    <StackPanel>
        <TextBox x:Name="headerText" TextChanged="headerText_TextChanged"/>
        <DockPanel>
            <CheckBox x:Name="chbox" Checked="chbox_Checked" Unchecked="chbox_Unchecked"/>
            <TextBox x:Name="TailText" IsEnabled="False" TextChanged="TailText_TextChanged"/>
        </DockPanel>
        
    </StackPanel>
</UserControl>
