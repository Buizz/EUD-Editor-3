<UserControl x:Class="GUI_Action_Count"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EUD_Editor_3"
             mc:Ignorable="d" 
            
             d:DesignHeight="450" d:DesignWidth="800">
    <DockPanel LastChildFill="False">
        <TextBox x:Name="countVal" DockPanel.Dock="Top" TextChanged="countVal_TextChanged" MouseWheel="countVal_MouseWheel"/>
        <CheckBox x:Name="ChBox" DockPanel.Dock="Top" Style="{StaticResource MaterialDesignCheckBox}" Checked="ChBox_Checked" Unchecked="ChBox_Unchecked">
            전체
        </CheckBox>
    </DockPanel>
</UserControl>
